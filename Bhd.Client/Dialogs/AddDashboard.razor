<MudDialog>
    <TitleContent>
        <MudText Typo="Typo.h6">
            New dashboard
        </MudText>
    </TitleContent>
    <DialogContent>
        <MudTextField @bind-Value="_dashboardName" Label="Name"/>
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Cancel">Cancel</MudButton>
        <MudButton Color="Color.Primary" OnClick="CreateDashboard">Create</MudButton>
    </DialogActions>
</MudDialog>