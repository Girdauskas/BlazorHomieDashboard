<MudDialog>
    <TitleContent>
        <MudText Typo="Typo.h6">
            New Dashboard
        </MudText>
    </TitleContent>
    <DialogContent>
        <MudTextField @bind-Value="_dashboardName" Label="Name" Required="true"/>
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Cancel">Cancel</MudButton>
        <MudButton Color="Color.Primary" OnClick="CreateDashboard">Create</MudButton>
    </DialogActions>
</MudDialog>