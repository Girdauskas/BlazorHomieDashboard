<MudDialog>
    <TitleContent>
        <MudText Typo="Typo.h6">
            Remove Dashboard
        </MudText>
    </TitleContent>
    <DialogContent>
        <MudText>Are you sure want to delete "@Dashboard.Name"?</MudText>
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Cancel">Cancel</MudButton>
        <MudButton Color="Color.Error" OnClick="Delete">Delete</MudButton>
    </DialogActions>
</MudDialog>