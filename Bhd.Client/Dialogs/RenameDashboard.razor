<MudDialog>
    <TitleContent>
        <MudText Typo="Typo.h6">
            Rename Dashboard
        </MudText>
    </TitleContent>
    <DialogContent>
        <MudTextField @bind-Value="_newDashboardName" Label="Name" Required="true" />
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Cancel">Cancel</MudButton>
        <MudButton Color="Color.Primary" OnClick="Rename">Rename</MudButton>
    </DialogActions>
</MudDialog>