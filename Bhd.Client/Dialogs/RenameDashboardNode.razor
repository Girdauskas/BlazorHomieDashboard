<MudDialog>
    <TitleContent>
        <MudText Typo="Typo.h6">
            Rename Node
        </MudText>
    </TitleContent>
    <DialogContent>
        <MudTextField @bind-Value="_newNodeName" Label="Name" Required="true" />
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Cancel">Cancel</MudButton>
        <MudButton Color="Color.Primary" OnClick="RenameNode">Rename</MudButton>
    </DialogActions>
</MudDialog>