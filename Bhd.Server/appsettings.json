{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        },
        "Console": {
            "FormatterName": "Simple",
            "FormatterOptions": {
                "SingleLine": false,
                "TimestampFormat": "yyyy-MM-ddTHH:mm:ss.fffZ ",
                "IncludeScopes": true
            }
        }
    },

    "AllowedHosts": "*"
}